using System;
class Operadora {
    static void Main() {
        Console.Write("Digite a quantidade de minutos utilizados: ");
        int minutos = int.Parse(Console.ReadLine());

        double valor = 50.0;
        if (minutos > 100) {
            valor += (minutos - 100) * 2.0;
        }

        Console.WriteLine($"Valor a pagar: R$ {valor:F2}");
    }
}
